#include <stdio.h>

int main(){
int a[2][3] ={{2,3,6},{3,4,5}};
int b[3][4] ={{1,3,5,7},{7,9,11,13},{15,17,19,21}};
 int c00=a[0][0]+b[0][0];
 int c01=a[0][1]+b[0][1];
 int c02=a[0][2]+b[0][2];
 int c10=a[1][0]+b[1][0];
 int c11=a[1][1]+b[1][1];
 int c12=a[1][2]+b[1][2];
 int ans=(c00+c01+c02+c10+c11+c12);
printf("%d+%d+%d+%d+%d+%d=%d",c00,c01,c02,c10,c11,c12,ans);
    return 0;
}
